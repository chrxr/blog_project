server {
    listen 80;
    server_name chrxr.com www.chrxr.com 18.220.139.29;

    location = /favicon.ico { 
        access_log off; log_not_found off; 
    }
    location /static/ {
        root /home/ubuntu/blog_project;
    }
    location /media/ {
        root /home/ubuntu/blog_project;
    }
    location / {
        include         uwsgi_params;
        uwsgi_pass      unix:/run/uwsgi/blog_project.sock;
    }
}

